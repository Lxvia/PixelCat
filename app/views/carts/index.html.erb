<h1>Carts#index</h1>
<p>Find me in app/views/carts/index.html.erb</p>


 <% @events.each do |event| %>
        <% if event.admin_id == current_user.id%>
          <div class="row d-flex justify-content-end">
            <div class="col-sm-9 d-flex">
              <div class="well m-5">
                <b><%= event.title %></b>
                <p class="text-decoration-none"><%= link_to "Voir l'Event", event_path(event), class: "btn btn-info" %></p>
              </div>
            </div>
          </div>
        <% else %>
        <% end %>
      <% end %>